{
  "name": "nats-laravel-echo",
  "version": "1.0.0",
  "description": "NATS driver for Laravel Echo",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "dev": "rollup -c -w",
    "test:auth": "node test-nats-auth.js",
    "test:simple": "node test.js",
    "test:env": "cross-env NATS_HOST=localhost NATS_PORT=53969 NATS_USER=local NATS_PASS=4elDZZTb7ofiN83BKjXOvhZvOuhUouhH NATS_DEBUG=true node examples/env-config.js",
    "prepare": "npm run build",
    "example": "node example.js"
  },
  "keywords": [
    "laravel",
    "echo",
    "nats",
    "broadcasting",
    "websocket"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "nats.ws": "^1.30.0"
  },
  "peerDependencies": {
    "laravel-echo": "^1.15.0 || ^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/node": "^25.0.6",
    "cross-env": "^7.0.3",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.0",
    "typescript": "^5.3.0"
  }
}
