  "keywords": [
    "laravel",
    "echo",
    "nats",
    "websocket",
    "realtime",
    "broadcasting"
  ],
  "author": "Benedict Mwanga",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mwangaben/nats-laravel-echo"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && rollup -c",
    "clean": "rm -rf dist",
    "test": "node test/test.js"
  },
  "dependencies": {
    "nats.ws": "^1.26.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
